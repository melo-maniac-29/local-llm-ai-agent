# Deployment used by `npx convex dev`
CONVEX_DEPLOYMENT=

# Convex URL - Keep this pointing to your cloud deployment
NEXT_PUBLIC_CONVEX_URL=

# LLM Configuration - Available on the client side
NEXT_PUBLIC_LLM_API_URL=http://localhost:1234
NEXT_PUBLIC_LLM_MODEL=  

# Server-side env variables
LLM_API_URL=http://127.0.0.1:1234
LLM_MODEL=  
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=500

NEXT_PUBLIC_APP_URL=http://localhost:3000

# Google API credentials for Calendar integration
GOOGLE_CLIENT_ID=   
GOOGLE_CLIENT_SECRET=   
GOOGLE_REDIRECT_URI=http://localhost:3000/api/auth/google/callback